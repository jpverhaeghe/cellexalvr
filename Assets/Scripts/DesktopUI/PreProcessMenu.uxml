    <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" style="align-items: center; justify-content: center; flex-direction: column; flex-grow: 1; border-left-width: 4px; border-right-width: 4px; border-top-width: 4px; border-bottom-width: 4px; border-top-left-radius: 4px; border-bottom-left-radius: 4px; border-top-right-radius: 4px; border-bottom-right-radius: 4px; background-color: rgba(255, 255, 255, 0); -unity-font-definition: resource(&apos;Fonts &amp; Materials/SpaceGrotesk-Regular SDF_fontasset&apos;); -unity-font: resource(&apos;Fonts &amp; Materials/SpaceGrotesk-Regular&apos;);">
