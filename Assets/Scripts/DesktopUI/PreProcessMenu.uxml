<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/Resources/StyleSheets/mainStyleSheet.uss?fileID=7433441132597879392&amp;guid=9c1933af9180dd647920416a67c9a28c&amp;type=3#mainStyleSheet" />
    <ui:VisualElement name="screen" style="text-shadow: 0 0 0 rgb(255, 255, 255); color: rgb(255, 255, 255); flex-grow: 1; background-color: rgba(0, 0, 0, 0); font-size: 14px; -unity-font-style: normal; letter-spacing: 7px; justify-content: space-between; align-items: center; -unity-font: resource(&apos;Fonts &amp; Materials/SpaceGrotesk-Regular&apos;); -unity-font-definition: resource(&apos;Fonts &amp; Materials/SpaceGrotesk-Regular SDF_fontasset&apos;);">
