            <ui:VisualElement name="output-container" style="width: 856px; height: 120px;">
                <ui:Label display-tooltip-when-elided="true" name="output-label" style="color: rgb(71, 255, 0); -unity-font-definition: url(&apos;project://database/Assets/Resources/Fonts%20&amp;%20Materials/SpaceGrotesk-Regular%20SDF_fontasset.asset?fileID=11400000&amp;guid=129b770ca1ffd9f409ff7a3813fb3b68&amp;type=2#SpaceGrotesk-Regular SDF_fontasset&apos;);" />
